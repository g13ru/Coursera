   "source": [
    "from functools import wraps\n",
    "from json import dumps\n",
    "\n",
    "\n",
    "def to_json(func):\n",
    "    @wraps(func)\n",
    "    def wrapper(*args, **kwargs):\n",
    "        return dumps(func(*args, **kwargs))\n",
    "\n",
    "    return wrapper\n",
    "\n"
   ]
